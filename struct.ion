struct Foo {
    bar: int
}

struct Person {
    age: int,
    foo: []Foo,
    name: string
}

fn do_something(x: Person) -> void {
    println(x);
}


fn main() -> void {
    var test := []Foo.[Foo.{0}, Foo.{2}];
    var x: Person = Person.{23, []Foo.[Foo.{5}, Foo.{10}], "John"};
    test[0].bar = 1;
    println(test[0].bar);
    println(x.foo[0].bar);
    x.foo[0].bar = 6;

    test[test[0].bar].bar = 53;

    do_something(x);
}