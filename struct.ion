struct Foo {
    bar: float
}

struct Person {
    age: int,
    foo: []Foo,
    name: string
}

fn do_something(x: Person) -> void {
    println(x);
}


fn main() -> void {
    var test := []Foo.[Foo.{5.3}, Foo.{10.3}];
    var x: Person = Person.{23, []Foo.[Foo.{5.3}, Foo.{10.3}], "John"};
    test[0].bar = 3.9;
    println(test[0].bar);
    println(x.foo[0].bar);
    x.foo[0].bar = 64.2;

    do_something(x);
}