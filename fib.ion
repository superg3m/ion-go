fn fib(x: int) -> int {
    if (x == 0 || x == 1) {
        return x;
    }

    return fib(x - 1) + fib(x - 2);
}

fn main() -> void {
    defer {
        defer print(fib(8) + "\n");
        print("Hello world\n");
    }

    defer print("THIS IS ACTUALLY BEFORE LAST DEFER!\n");

    if (false || fib(100) > 4) {
        print("First!\n");
        return;
    }

    print("NOT HAPPENING\n");
}