var cool_factor := 7.0;

fn do_something(arr: float[], y: int, z: float) -> float {
    var x := 5.4 + y + z;
    x = x * 2;

    for (var i := 0; i < len(arr); i = i + 1;) {
        x = x + arr[i];
    }

    return x + cool_factor;
}

fn main() -> void {
    var arr2: float[][] = [[2.5, 6.5]];
    print(arr2[0][1]);

    var test := 5.5;
    var arr: float[] = [1.5, test];
    var x: float = 64 + do_something(arr, 6, 5.0);

    print(x);
    print(arr);
}