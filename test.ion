var cool_factor := 7.0;

fn do_something(arr: []float, y: int, z: float) -> float {
    var x := 5.4 + y + z;
    x = x * 2;

    for (var i := 0; i < len(arr); i = i + 1) {
        x = x + arr[i];
        if (x <= 30) {
            println("Hello: " + x);
            continue;
        }

        print("World!\n");
    }

    x = 3.0;
    if (x >= 1.0) {
        return x;
    } else {
        x = 10.0;
    }

    return x + cool_factor;
}

fn main() -> void {
    var arr2 := [][]float.[[1.54, 5.4], [4.2]];
    for (var i := len(arr2) - 1; i >= 0; i = i - 1) {
        println(arr2[i]);
    }

    var test := -(5.5);
    var arr := []float.[1.5, test];
    print(arr);

    var kldfjdskljf: float = 64 + do_something(arr, 6, cast(float)5.0);
    println(kldfjdskljf);

    var x := cast(string)5.3 + 5;
    println(x);
    println(len(x));
}