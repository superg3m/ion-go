fn do_something(y: int, z: float) -> float {
    var x := 5.4 + y + z;
    x = x * 2;

    return x;
}

fn main() -> void {
    var x: float = 64 + do_something(6, 5.0);

    var z: float[] = float.[1.5, x];

    for (var i := 0; i < len(z); i = i + 1;) {
        print(z[i]);
    }

    print(z);
    print(x);
}

// return type on void