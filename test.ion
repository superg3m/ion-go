var cool_factor := 7.0;

fn do_something(arr: []float, y: int, z: float) -> float {

    var x := 5.4 + y + z;
    x = x * 2;

    for (var i := 0; i < len(arr); i = i + 1) {
        x = x + arr[i];

        if (x <= 30) {
            defer print("AFTER THE CONTINUE!\n");
            print("Hello: " + x + "\n");
            continue;
        }

        print("World!\n");
    }

    x = 3.0;
    if (x >= 1.0) {
        return x;
    } else {
        x = 10.0;
    }

    return x + cool_factor;
}

fn main() -> void {
    var arr2 := [][]float.[[1.54, 5.4], [4.2]];
    println(arr2);
    println(arr2[0]);
    println(arr2[0][1]);

    var test := -(5.5);
    var arr := []float.[1.5, test];
    var kldfjdskljf: float = 64 + do_something(arr, 6, cast(float)5.0);

    println(kldfjdskljf);
    println(arr);

    var x := cast(string)5.3 + 5;
    println(x);
    println(len(x));
}