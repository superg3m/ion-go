var cool_factor := 7.0;

fn do_something(arr: []float, y: int, z: float) -> float {

    var x := 5.4 + y + z;
    x = x * 2;

    for (var i := 0; i < len(arr); i = i + 1) {
        x = x + arr[i];

        if (x <= 35) {
            defer print("AFTER THE CONTINUE!\n");
            print("Hello: " + x + "\n");
            continue;
        }

        print("World!\n");
    }

    x = 3.0;
    if (x >= 1.0) {
        return x;
    } else {
        x = 10.0;
    }

    return x + cool_factor;
}

fn main() -> void {
    var arr2 := [][]float.[[1.54, 5.4], [4.2]];
    print(arr2);
    print("\n");
    print(arr2[0]);
    print("\n");
    print(arr2[0][1] + "\n");

    var test := 5.5;
    var arr := []float.[1.5, test];
    var kldfjdskljf: float = 64 + do_something(arr, 6, 5.0);

    print(kldfjdskljf + "\n");
    print(arr);
    print("\n");
}